<button class=\"btn\" @click=\"cambiarContador('-1')\" type=\"button\" name=\"button\">-</button>\n          <input id=\"cantidad\" type=\"text\" name=\"name\" :value=\"contador\">\n          <button class=\"btn\" @click=\"cambiarContador('1')\" type=\"button\" name=\"button\">+</button>\n      </div>\n      <div >\n        <p id=\"so\">Instalación del Sistema Operativo: ( 60€ )<input type=\"checkbox\" id=\"check\" @change=\"visible()\" /><br></p>\n        <select class=\"oculto\" >\n          <option value=\"win10\">Windows 10</option>\n          <option value=\"win11\">Windows 11</option>\n          <option value=\"lin\">Linux</option>\n        </select> \n      </div>\n      <br>\n      <div id=\"botones\">\n      <button id=\"boton\"> AÑADIR AL CARRITO</button><button id=\"comprar\"> COMPRAR</button></div>\n  </section>\n</template>\n\n<script>\nexport default {\n  Name: 'ProductoDatos',\n    data () {\n        return {\n            contador: 1\n        }\n    },\n    methods: {\n        cambiarContador(num) {\n            this.contador += +num;\n            if (!isNaN(this.contador) && (this.contador < 0)) {\n              this.contador = 0\n            }\n        },\n        visible(){\n          let check=document.querySelector(\"#check\");\n          if(check.checked){\n            document.querySelector(\"select\").style.display=\"block\"\n            let h2=document.querySelector(\"h2\")\n            h2.innerHTML=\"1443,22€\"\n           \n          }else {\n            document.querySelector(\"select\").style.display='none';\n\n            let h2=document.querySelector(\"h2\")\n            h2.innerHTML=\"1383,22€\"\n            \n            \n          }\n        }\n    }\n\n\n}\n</script>\n\n<style scoped>\n.oculto{\n  display: none;\n}\n#so{\n  margin: 1em 0;\n}\n#sin-iva{\n  font-size: 0.5em;\n  color: rgb(1, 0, 36);\n  font-weight: 300;\n  margin-left: 1em;\n}\n\nsection#producto-datos {\n display: flex;\n flex-direction: column;\n align-items: center;\n justify-content: center;\n margin-top: -10em;\n margin-left: 5em;\n}\nh1 {\n  color: rgb(3, 0, 34);\n  font-size: 1.5em;\n}\nh2 {\n  font-size:1.7rem;\n  color: rgb(255, 123, 0);\n}\n\n#wrapper {\n  height: 2em;\n  display: flex;\n}\n\n#cantidad {\n  border: none;\n  text-align: center;\n  width: 2rem;\n  font-size: 1.2rem;\n  border: 1px solid #d8d8d8;\n}\n.btn {\n  border: 1px solid #d8d8d8;\n  width: 1.8rem;\n}\nbutton:focus, input:focus {\n  outline: 0;\n}\n#botones{\n  display: flex;\n}\n#boton{\nmargin-top: 2em;\npadding: 0.7em;\nfont-size: 1em;\nbackground-color: rgb(255, 123, 0);\ncolor: white;\nfont-weight: bold;\nmargin-right: 0.5em;\n}\n#comprar{\n  margin-top: 2em;\npadding: 0.7em;\nfont-size: 1em;\nbackground-color: white;\ncolor: rgb(255, 123, 0);\nfont-weight: bold;\nmargin-left: 0.5em;\n}\n#boton:hover{\n\nbackground-color: white;\ncolor: rgb(255, 123, 0);\nborder-color: rgb(255, 123, 0);\n\n}\n#comprar:hover{\n\nbackground-color: rgb(255, 123, 0);\ncolor: white;\nborder-color: white;\n}\nselect{\n  \npadding: 0.7em;\n\nbackground-color: white;\ncolor: rgb(255, 123, 0);\nfont-weight: bold;\nmargin-left: 0.5em;\nfont-size: 1em;\n}\n</style>"],"sourceRoot":""}]}